{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information"
    },
    "WriteTo": [
      {
        "Name": "Console"
      }
    ]
  },
  "ACS": [
    {
      "Type": "JsonFile",
      "Optional": true,
      "Path": "appsettings.Development.Extensions.json"
    }
  ],
  "Mssql": {
    "ConnectionString": "Data Source=0.tcp.ap.ngrok.io,${TM_SQL_PORT};database=TestMaker.UserService;Persist Security Info=True;User ID=${TM_SQL_USER:sa};Password=${TM_SQL_PASSWORD}"
  },
  "Mongodb": {
    "ConnectionString": "mongodb://${TM_MONGO_USER:root}:${TM_MONGO_PASSWORD}@${TM_MONGO_HOST:0.tcp.ap.ngrok.io}:${TM_MONGO_PORT:27017}",
    "Database": "TestMaker_UserService"
  },
  "Server": {
    "IdentityServer": "http://localhost:60001"
  },
  "InfrastructurePath": "../TestMaker.UserService.Infrastructure/bin/Debug/net6.0/TestMaker.UserService.Infrastructure.dll"
}
